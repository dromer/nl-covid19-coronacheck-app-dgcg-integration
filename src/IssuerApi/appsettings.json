{
  "Logging": {
    "LogLevel": {
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Certificates": {
    "CmsSigning": {
      "Path": "sign.p12",
      "Password": "123456",
